{
    "type": "container",
    "id": "update_root",
    "style": {
        "width": "100%",
        "height": 100,
        "flexDirection": "column"
    },
    "children": [
        {
            "type": "text",
            "id": "counter_text",
            "style": {
                "width": "100%",
                "height": "auto",
                "fontSize": 24
            },
            "props": {
                "text": "${count}"
            }
        },
        {
            "type": "text",
            "id": "time_text",
            "style": {
                "width": "100%",
                "height": "auto",
                "fontSize": 12
            },
            "props": {
                "text": "${timestamp}"
            }
        }
    ]
}
